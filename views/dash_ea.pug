h1
  span.blue
  |  Welcome
  span.blue
  span.yellow     #{user_fullname}

br
br

table.container
  thead
    tr
      th
        h1 Elections are Live
      th
        h1 #{election_status}
      th
        a.btn.btn-primary.verify1.w-75(href='#' onclick='switchElections()') Start / Stop
      th
        a.btn.btn-primary.verify1.w-75(href='/logOut') Logout

table.container
  thead
    tr
      th
        h1 USER NAME
      th
        h1 TYPE
      th
        h1 VERIFIED
      th
        h1 ACTION
  each val in user_details
    tbody
      tr
        td #{val.username}
        td #{val.role}
        td #{val.is_verified}
        td
          a.btn.btn-primary.verify.w-75(href='#' onclick='verify_user("'+val.username+'","'+val.role+'")') Verify
